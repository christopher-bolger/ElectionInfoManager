<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="root" prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="electionInfoManager.view.electionView">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                     <children>
                        <ListView fx:id="electionDetailsListView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                        <TableView fx:id="winnersView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                          <columns>
                              <TableColumn fx:id="positionColumn" maxWidth="1.7976931348623157E308" minWidth="75.0" prefWidth="75.0" text="Position" />
                            <TableColumn fx:id="nameColumn" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="100.0" text="Name" />
                            <TableColumn fx:id="affiliationColumn" maxWidth="1.7976931348623157E308" minWidth="100.0" prefWidth="100.0" text="Affiliation" />
                              <TableColumn fx:id="votesColumn" maxWidth="1.7976931348623157E308" minWidth="85.0" prefWidth="85.0" text="Total Votes" />
                          </columns>
                        </TableView>
                        <ButtonBar prefHeight="40.0" prefWidth="200.0">
                          <buttons>
                            <Button mnemonicParsing="false" onAction="#openCandidateList" text="Edit Candidate List" />
                              <Button layoutX="256.0" layoutY="18.0" mnemonicParsing="false" onAction="#viewSelectedCandidate" text="View Candidate" />
                          </buttons>
                        </ButtonBar>
                     </children>
                  </VBox>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
